div#tcal th
 {
	border:1px solid silver;
	border-collapse:collapse;
	text-align:center;
	font-family:Verdana, Tahoma, Arial;
	font-size:10px;
	background-color:gray;
	color:#FFF;
	padding:3px 0;
}

/* date cells */
