/* calendar container element */
div#tcal
 {
	position:absolute;
	visibility:hidden;
	z-index:100;
	width:158px;
	padding:2px 0 0;
}

/* all tables in calendar */
div#tcal table
 {
	width:100%;
	border:1px solid silver;
	border-collapse:collapse;
	background-color:#FFF;
}

/* navigation table */
div#tcal table.ctrl
 {
	border-bottom:0;
}

/* navigation buttons */
div#tcal table.ctrl td
 {
	width:15px;
	height:20px;
}

/* month year header */
